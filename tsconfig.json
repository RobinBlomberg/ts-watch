{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": [
    "**/*.js",
    "types.d.ts"
  ]
}
